                </Card>
              )}

              {/* Recomendaciones Estratégicas */}
              {analisisPredictivo.recomendacionesEstrategicas && analisisPredictivo.recomendacionesEstrategicas.length > 0 && (
                <Card className="border-blue-200">
                  <CardHeader>
                    <CardTitle className="flex items-center gap-2">
                      <Target className="h-5 w-5 text-blue-600" />
                      Recomendaciones Estratégicas
                    </CardTitle>
                  </CardHeader>
                  <CardContent>
                    <div className="space-y-3">
                      {analisisPredictivo.recomendacionesEstrategicas.map((rec, index) => (
                        <div key={index} className="p-3 bg-blue-50 rounded-lg border border-blue-200">
                          <div className="flex items-start justify-between mb-2">
                            <div>
                              <Badge variant={
                                rec.prioridad === 'alta' ? 'destructive' :
                                rec.prioridad === 'media' ? 'default' : 'secondary'
                              } className="mb-2">
                                {rec.prioridad}
                              </Badge>
                              <Badge variant="outline" className="ml-2">{rec.categoria}</Badge>
                            </div>
                          </div>
                          <p className="font-medium text-sm mb-1">{rec.recomendacion}</p>
                          <p className="text-xs text-muted-foreground">
                            <strong>Impacto esperado:</strong> {rec.impactoEsperado}
                          </p>
                        </div>
                      ))}
                    </div>
                  </CardContent>
                </Card>
              )}

              {/* Factores de Riesgo */}
              {analisisPredictivo.factoresRiesgo && analisisPredictivo.factoresRiesgo.length > 0 && (
                <Card className="border-red-200">
                  <CardHeader>
                    <CardTitle className="flex items-center gap-2">
                      <Shield className="h-5 w-5 text-red-600" />
                      Factores de Riesgo
                    </CardTitle>
                  </CardHeader>
                  <CardContent>
                    <div className="space-y-3">
                      {analisisPredictivo.factoresRiesgo.map((riesgo, index) => (
                        <div key={index} className="p-3 bg-red-50 rounded-lg border border-red-200">
                          <div className="flex items-start justify-between mb-2">
                            <p className="font-medium text-sm">{riesgo.factor}</p>
                            <Badge variant={
                              riesgo.impacto === 'alto' ? 'destructive' : 'default'
                            }>
                              {riesgo.impacto} impacto
                            </Badge>
                          </div>
                          <div className="flex items-center justify-between">
                            <span className="text-xs text-muted-foreground">Probabilidad: {riesgo.probabilidad}%</span>
                            <Progress value={riesgo.probabilidad} className="h-1 w-32" />
                          </div>
                          <p className="text-xs text-muted-foreground mt-2">
                            <strong>Mitigación:</strong> {riesgo.mitigacion}
                          </p>
                        </div>
                      ))}
                    </div>
                  </CardContent>
                </Card>
              )}
            </div>
          ) : (
            <Card>
              <CardContent className="p-12 text-center">
                <p className="text-muted-foreground">No hay datos predictivos disponibles</p>
              </CardContent>
            </Card>
          )}
        </TabsContent>
      </Tabs>
    </div>
  );
};

export default EstadisticasDashboard;
